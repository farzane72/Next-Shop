{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/public/*": ["public/*"],
      "@/app/*": ["src/app/*"],
      "@/utils/*": ["src/utils/*"],
      "@/layout/*": ["src/components/layout/*"],
      "@/module/*": ["src/components/module/*"],
      "@/template/*": ["src/components/template/*"],
      "@/providers/*": ["src/providers/*"],
      "@/api/*": ["src/app/api/*"],
      "@/hooks/*": ["src/core/hooks/*"],
      "@/services/*": ["src/core/services/*"],
      "@/tailwind/*": ["src/tailwind/*"],
      "@/redux/*": ["src/redux/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/redux/Providers.tsx", "src/redux/store.ts", "src/redux/features/account/loginSlice.ts"],
  //"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/core/services/privateAxios.js", "src/core/services/publicAxios.js"],
  "exclude": ["node_modules"]
}
